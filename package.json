  },
  "homepage": "https://github.com/NodeSecure/vis-network#readme",
  "dependencies": {
    "pretty-bytes": "^6.0.0",
    "vis-data": "^7.1.4",
    "vis-network": "^9.1.2"
  },
  "devDependencies": {
    "@nodesecure/eslint-config": "^1.4.1",
    "@nodesecure/flags": "^2.4.0",
    "@nodesecure/scanner": "^3.7.0",
    "c8": "^7.12.0",
